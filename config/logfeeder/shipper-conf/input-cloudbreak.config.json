{
  "input": [
    {
      "type": "cloudbreak_server",
      "rowtype": "service",
      "path": "/logs/cloudbreak-*.log"
    }
  ],
  "filter": [
    {
      "filter": "grok",
      "conditions": {
        "fields": {
          "type": [
            "cloudbreak_server"
          ]
        }
      },
      "multiline_pattern": "^(%{DATESTAMP:logtime})",
      "message_pattern": "(?m)^%{DATESTAMP:logtime}%{SPACE}\\[%{DATA:thread_name}\\]%{SPACE}%{JAVAMETHOD:method}:%{NUMBER:line}%{SPACE}%{LOGLEVEL:level}%{SPACE}%{JAVACLASS:logger_name}%{SPACE}-%{SPACE}\\[owner:%{DATA:cb_owner}\\]%{SPACE}\\[type:%{DATA:cb_type}\\]%{SPACE}\\[id:%{DATA:cb_id}\\]%{SPACE}\\[name:%{DATA:cb_name}\\]%{SPACE}\\[flow:%{DATA:cb_flow}\\]%{SPACE}%{GREEDYDATA:log_message}",
      "post_map_values": {
        "logtime": {
          "map_date": {
            "target_date_pattern": "yyyy-MM-dd HH:mm:ss,SSS"
          }
        },
        "cb_owner":{
          "map_fieldvalue":{
            "pre_value":"",
            "post_value":"unknown"
          }
        },
        "cb_type":{
          "map_fieldvalue":{
            "pre_value":"",
            "post_value":"unknown"
          }
        },
        "cb_id":{
          "map_fieldvalue":{
            "pre_value":"",
            "post_value":"unknown"
          }
        },
        "cb_name":{
          "map_fieldvalue":{
            "pre_value":"",
            "post_value":"unknown"
          }
        },
        "cb_flow":{
          "map_fieldvalue":{
            "pre_value":"",
            "post_value":"unknown"
          }
        }
      }
    }
  ]
}
