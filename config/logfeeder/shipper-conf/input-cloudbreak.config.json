{
  "input": [
    {
      "type": "cloudbreak_server",
      "rowtype": "service",
      "path": "/logs/*.log"
    }
  ],
  "filter": [
    {
      "filter": "grok",
      "conditions": {
        "fields": {
          "type": [
            "cloudbreak_server"
          ]
        }
      },
      "multiline_pattern": "^(%{DATESTAMP:logtime})",
      "message_pattern": "(?m)^%{DATESTAMP:logtime}%{SPACE}\\[%{DATA:thread_name}\\]%{SPACE}%{JAVAMETHOD}:%{NUMBER:line}%{SPACE}%{LOGLEVEL:level}%{SPACE}%{JAVACLASS:logger_name}%{SPACE}-%{SPACE}%{GREEDYDATA:log_message}",
      "post_map_values": {
        "logtime": {
          "map_date": {
            "target_date_pattern": "yyyy-MM-dd HH:mm:ss,SSS"
          }
        }
      }
    }
  ]
}
