{
  "input": [
    {
      "type": "cloudbreak_web",
      "rowtype": "service",
      "path": "/logs/cb-web-*.log"
    }
  ],
  "filter": [
    {
      "filter": "grok",
      "conditions": {
        "fields": {
          "type": [
            "cloudbreak_web"
          ]
        }
      },
      "multiline_pattern": "^(%{TIMESTAMP_ISO8601:logtime})",
      "message_pattern": "(?m)^%{TIMESTAMP_ISO8601:logtime}%{SPACE}%{LOGLEVEL:level}%{SPACE}\\[owner:%{DATA:cb_owner}\\]%{SPACE}\\[email:%{DATA:cb_email}\\](%{SPACE}%{DATA:cb_address}%{SPACE}-|)%{SPACE}%{GREEDYDATA:log_message}",
      "post_map_values": {
        "logtime": {
          "map_date": {
            "target_date_pattern": "yyyy-MM-dd'T'HH:mm:ss.SSSXX"
          }
        },
        "cb_owner":{
          "map_fieldvalue":{
            "pre_value":"",
            "post_value":"unknown"
          }
        },
        "cb_email":{
          "map_fieldvalue":{
            "pre_value":"",
            "post_value":"unknown"
          }
        }
      }
    }
  ]
}