{
  "input": [
    {
      "type": "consul",
      "rowtype": "service",
      "path": "/logs/consul-*.log"
    }
  ],
  "filter": [
    {
      "filter": "grok",
      "conditions": {
        "fields": {
          "type": [
            "consul"
          ]
        }
      },
      "multiline_pattern": "^(%{DATESTAMP:logtime})",
      "message_pattern": "(?m)^%{DATESTAMP:logtime}%{SPACE}\[%{LOGLEVEL:level}\]%{SPACE}%{GREEDYDATA:log_message}",
      "post_map_values": {
        "logtime": {
          "map_date": {
            "target_date_pattern": "yyyy-MM-dd HH:mm:ss,SSS"
          }
        }
      }
    }
  ]
}